<?xml version="1.0" ?>
<launch>
  <arg name="sim" default="false"/>
  <arg name="pipeline" default="ompl"/>
  <arg name="rviz" default="true"/>

  <!-- PAPRAS HW-->
  <arg name="usb_port" default="/dev/ttyUSB0"/>
  <arg name="baud_rate" default="1000000"/>
  <arg name="yaml_file" default="$(find papras_table)/config/two_7dof_papras_demo.yaml"/>


    <!-- startup robot_description related -->
  <group ns="followers"> 
    <param name="robot_description" command="$(find xacro)/xacro '$(find papras_table)/description/urdf/teleop_table_7dof.urdf.xacro'"/>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
        <param name="tf_prefix" value="followers"/>
    </node>

    <node pkg="papras_controls" type="papras_control_node" name="papras_control_node" output="screen">
        <param name="usb_port" value="$(arg usb_port)"/>
        <param name="baud_rate" value="$(arg baud_rate)"/>    
        <param name="yaml_file" value="$(arg yaml_file)"/>
    </node>

    <!-- startup ROS control related -->
    <include file="$(find papras_controls)/launch/controller_utils.launch"/>
    <rosparam file="$(find papras_controls)/config/joint_trajectory_controller_7dof.yaml" command="load"/>
    <node name="arm1_controller_spawner" pkg="controller_manager" type="controller_manager" respawn="false" output="screen" args="spawn arm3_controller arm4_controller gripper3_controller gripper4_controller"/>

    <include file="$(find two_7dof_papras_moveit_config)/launch/move_group.launch">
      <arg name="pipeline" value="$(arg pipeline)"/>
      <arg name="load_robot_description" value="true" />
      <remap from="/joint_states" to="/followers/joint_states" />
    </include>
  </group>

  <group if="$(arg rviz)">
    <node name="$(anon rviz)" launch-prefix="" pkg="rviz" type="rviz" respawn="false"
        args="-d $(find papras_table)/config/teleoperation_table.rviz" output="screen">
    </node>
  </group>


</launch>
